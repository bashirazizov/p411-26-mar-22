@model HomeViewModel

<section id="hero" class="d-flex align-items-center">

    <div class="container">
        <div class="row">
            <div class="col-lg-6 pt-5 pt-lg-0 order-2 order-lg-1 d-flex flex-column justify-content-center">
                <h1 data-aos="fade-up">@Model.Banner.Title</h1>
                <h2 data-aos="fade-up" data-aos-delay="400">@Model.Banner.Text</h2>
                <div data-aos="fade-up" data-aos-delay="800">
                    <a href="@Model.Banner.BtnLink" class="btn-get-started scrollto">@Model.Banner.BtnText</a>
                </div>
            </div>
            <div class="col-lg-6 order-1 order-lg-2 hero-img" data-aos="fade-left" data-aos-delay="200">
                <img src="assets/img/@Model.Banner.Img" class="img-fluid animated" alt="">
            </div>
        </div>
    </div>

</section><!-- End Hero -->

<main id="main">


    <section id="clients" class="clients clients mb-5">
        <div class="container">

            <div class="row justify-content-center">

                @foreach (AppUser user in Model.Users)
                {
                    <div class="col-lg-2 col-md-4 col-6 d-block">
                        @if (!string.IsNullOrEmpty(user.ProfilePhoto))
                        {
                            <img src="~/assets/img/users/@user.ProfilePhoto" class="img-fluid" data-aos="zoom-in" style="border-radius:100%">
                        }
                        else
                        {
                            <img src="~/assets/img/defPerson.jpg" class="img-fluid" data-aos="zoom-in" style="border-radius:100%">
                        }
                        <p class="text-center">
                            <a href="/account/index/@user.UserName">
                                @user.FullName
                            </a>
                        </p>
                    </div>
                }

            </div>

        </div>
    </section>
    <!-- ======= Clients Section ======= -->
    <section id="clients" class="clients clients">
        <div class="container">

            <div class="row justify-content-center">

                @foreach (Partner item in Model.Partners)
                {
                    <div class="col-lg-2 col-md-4 col-6">
                        <img src="assets/img/clients/@item.Img" class="img-fluid" alt="@item.Name" data-aos="zoom-in">
                    </div>

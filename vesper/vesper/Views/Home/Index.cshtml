                <div class="col-lg-12 d-flex justify-content-center">
                    <ul id="portfolio-flters">
                        <li data-filter="*" class="filter-active">All</li>
                        @foreach (Category item in Model.Categories)
                        {
                            <li data-filter=".filter-@item.Name.ToLower()">@item.Name</li>
                        }
                    </ul>
                </div>
            </div>

            <div class="row portfolio-container isotoped" data-aos="fade-up" data-aos-delay="400">

                @foreach (Project item in Model.Projects)
                {
                    <div class="col-lg-4 col-md-6 portfolio-item
                        @foreach (ProjectToCategory ptc in item.ProjectToCategories)
                        {
                            @("filter-"+ptc.Category.Name.ToLower()+" ")
                        }
                        ">
                        <div class="portfolio-wrap">
                            <img src="assets/img/portfolio/@item.Image" class="img-fluid" alt="">
                            <div class="portfolio-info">
                                <h4>@item.Name</h4>
                                <p>
                                    @foreach (ProjectToCategory ptc in item.ProjectToCategories)
                                    {
                                        @if (item.ProjectToCategories.IndexOf(ptc) == item.ProjectToCategories.Count - 1)
                                        {
                                            @(ptc.Category.Name)
                                        }
                                        else
                                        {
                                            @(ptc.Category.Name+", ")
                                        }
                                    }
                                </p>
                                <div class="portfolio-links">
                                    <a href="assets/img/portfolio/@item.Image" data-gallery="portfolioGallery" class="portfolio-lightbox" title="@item.Name"><i class="bx bx-plus"></i></a>
                                    <a href="/Projects/Info/@item.Id" title="More Details"><i class="bx bx-link"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            </div>
            <div class="text-center">
